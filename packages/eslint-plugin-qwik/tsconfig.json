{
  "compilerOptions": {
    "incremental": true,
    "target": "ES2022",
    "module": "ES2022",
    "lib": ["ES2022", "DOM"],
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "jsxImportSource": "@builder.io/qwik",
    "declaration": true,
    "isolatedModules": true,
    "strict": true,
    "moduleResolution": "Bundler",
    "baseUrl": ".",
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "sourceRoot": ".",
    "inlineSourceMap": true,
    "inlineSources": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "typeRoots": ["./types", "./node_modules/@types"],
    "types": ["jsx-ast-utils"]
  },
  "include": ["src/**/*.ts", "file.tsx", "react.tsx", "src/routes/index.tsx"]
}
